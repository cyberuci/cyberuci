		"@sanity/client": "7.4.0",
		"@sanity/image-url": "1.1.0",
		"@sveltejs/adapter-cloudflare": "7.2.4",
		"@sveltejs/enhanced-img": "0.6.0",
		"@sveltejs/kit": "2.21.2",
		"@sveltejs/vite-plugin-svelte": "5.1.0",
		"@types/debounce": "1.2.4",
		"@typescript-eslint/eslint-plugin": " 8.33.1",
